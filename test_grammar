/*
enum Token {
    "e" => Token::E,
    "c" => Token::C,
    "d" => Token::D,
}

pub Entry: Expr = {
    <x:X> "c" => Expr::C(x),
    <y:Y> "d" => Expr::D(y),
};

X: usize = {
    "e" <x:X> => x + 1,
    "e" => 1,
};

Y: usize = {
    "e" <y:Y> => y + 1,
    "e" => 1,
};
*/

enum Token {
    "a" => Token::A,
    "b" => Token::B,
    "c" => Token::C,
    "d" => Token::D,
    "e" => Token::E,
}

pub Entry: Expr = {
    "a" X "d",
    "a" Y "c",
    "b" Y "d",
    "b" X "c",
};

X: usize = {
    "e" X,
    "e",
};

Y: usize = {
    "e" Y,
    "e",
};
